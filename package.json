{
  "name": "react-cordova",
  "displayName": "React Cordova",
  "version": "0.1.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "homepage": "./",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot --config webpack/webpack.dev.js",
    "build": "webpack --mode production --config webpack/webpack.prod.js",
    "add:browser": "cordova platform add browser",
    "add:android": "cordova platform add android",
    "add:ios": "cordova platform add ios",
    "browser": "cordova run browser",
    "android": "cordova run android",
    "ios": "cordova run ios"
  },
  "keywords": [
    "ecosystem:cordova",
    "react"
  ],
  "author": "VIMAL RADHAKRISHNAN",
  "license": "Apache-2.0",
  "dependencies": {
    "child_process": "^1.0.2",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.1.1",
    "fs": "0.0.1-security",
    "history": "^4.10.1",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^5.1.2",
    "react-svg": "^11.0.15",
    "rimraf": "^3.0.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "html-webpack-plugin": "^4.0.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "image-webpack-loader": "^6.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "ios",
      "browser"
    ]
  }
}